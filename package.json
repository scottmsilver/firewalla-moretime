{
  "name": "fw",
  "version": "1.0.0",
  "description": "A Python CLI tool to manage internet access time extensions for your kids on Firewalla devices. This tool allows you to automatically grant additional internet time when your kids run out of their daily quota, with full request logging.",
  "main": "setup_firewalla_api.js",
  "scripts": {
    "test": "node server/tests/qr_validation.test.js",
    "test:qr": "node server/tests/test_qr_parser.js",
    "test:connection": "node server/tests/qr_connection.test.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm start",
    "start": "node server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "googleapis": "^164.0.0",
    "jimp": "^1.6.0",
    "jsqr": "^1.4.0",
    "multer": "^2.0.2",
    "node-firewalla": "^1.3.1",
    "nodemailer": "^7.0.9",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pngjs": "^7.0.0",
    "qrcode-reader": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10"
  }
}
